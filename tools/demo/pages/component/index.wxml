<scroll-view scroll-y class="scroll-page">
    <view class="main">
        <view class="box flex swatch justify-around flex-wrap">
            <view class="item flex justify-between align-center swatch-color bg-{{colour[index]}}" wx:for="{{list}}" wx:key="key">
                <view class="left">
                    <text class="text-shadow text-white {{item.icon}}"></text>
                    <text class="cn text-shadow text-white">{{item.cn}}</text>
                </view>
                <view class="right">
                    <text class="en capitalize text-shadow text-white">{{item.en}}</text>
                    <view class="lines">
                        <text class="line line-1"></text>
                        <text class="line line-2"></text>
                        <text class="line line-3"></text>
                        <text class="line line-4"></text>
                        <text class="line line-5"></text>
                        <text class="line line-6"></text>
                        <text class="line line-7"></text>
                        <text class="line line-8"></text>
                        <text class="line line-9"></text>
                        <text class="line line-1"></text>
                        <text class="line line-2"></text>
                        <text class="line line-3"></text>
                        <text class="line line-4"></text>
                        <text class="line line-5"></text>
                        <text class="line line-6"></text>
                        <text class="line line-7"></text>
                        <text class="line line-8"></text>
                        <text class="line line-9"></text>
                    </view>
                </view>
            </view>
        </view>
    </view>
</scroll-view>
