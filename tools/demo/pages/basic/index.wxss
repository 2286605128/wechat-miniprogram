page {
  --c-bg: #2e1f47;
  background: var(--c-bg);
}
.main {
  width: 100%;
  min-height: calc(100vh - 500rpx);
  margin-top: 130rpx;
  padding-bottom: 100rpx;
}
.main .box {
  background: #f7f8fb;
  background-size: 17rpx 100rpx, 116rpx 100rpx, 17rpx 100rpx;
  background-repeat: no-repeat;
  background-position: 10rpx 0rpx,26rpx 0rpx,142rpx 0rpx;
  border-radius: 60rpx 60rpx 0 0;
  padding: 36rpx 24rpx;
  box-shadow: 0 -18rpx 8rpx -18rpx rgba(0, 0, 0, 0.1);
  position: relative;
  z-index: 9;
}
.main .box .item {
  width: 300rpx;
  background: #fff;
  margin-top: 40rpx;
  border-radius: 28rpx;
  overflow: hidden;
  box-shadow: 5rpx 5rpx 16rpx 0 rgba(0, 0, 0, 0.1);
}
.main .box .item:nth-child(1),
.main .box .item:nth-child(2) {
  margin-top: 0;
}
.main .box .item:last-child {
  box-shadow: none;
  background: transparent;
  border-radius: 0;
}
.main .box .item .top {
  height: 160rpx;
  position: relative;
  text-align: center;
  line-height: 66rpx;
  overflow: hidden;
  box-shadow: 0 1rpx 4rpx rgba(0, 0, 0, 0.3), 0 0 40rpx rgba(0, 0, 0, 0.1) inset;
}
.main .box .item .top .en {
  margin-top: 12rpx;
}
.main .box .item .top .mask {
  width: 100%;
  height: 50rpx;
  position: absolute;
  bottom: 0;
  left: 0;
}
.main .box .item .bottom {
  padding: 20rpx 0;
  background: #fff;
}
.main .box .item .bottom .ani-btn {
  width: 56%;
  font-size: 26rpx;
  line-height: 2;
}
.paw,
.tail {
  position: absolute;
  left: 0;
  right: 0;
  margin: auto;
}
.paw .paw_left,
.tail .paw_left,
.paw .paw_right,
.tail .paw_right {
  position: absolute;
  z-index: 5;
  width: 30rpx;
  height: 20rpx;
  border-top-left-radius: 100%;
  border-top-right-radius: 100%;
  background: #b8a392;
}
.paw {
  width: 130rpx;
  z-index: 10;
}
.paw .paw_left {
  left: 0;
  top: -10rpx;
}
.paw .paw_right {
  right: 0rpx;
  top: -10rpx;
}
.paw .paw_left__part,
.paw .paw_right__part {
  width: 10rpx;
  height: 10rpx;
  background: #b8a392;
  border-radius: 10rpx;
  position: absolute;
  bottom: -4rpx;
}
.paw .paw_left__part:nth-of-type(1),
.paw .paw_right__part:nth-of-type(1) {
  left: 0rpx;
}
.paw .paw_left__part:nth-of-type(2),
.paw .paw_right__part:nth-of-type(2) {
  left: 10rpx;
}
.paw .paw_left__part:nth-of-type(3),
.paw .paw_right__part:nth-of-type(3) {
  left: 20rpx;
}
.puppy {
