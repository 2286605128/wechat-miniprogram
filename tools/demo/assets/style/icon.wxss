@keyframes an-icon-spin {
    0% {
        -webkit-transform: rotate(0);
        transform: rotate(0);
    }

    100% {
        -webkit-transform: rotate(359deg);
        transform: rotate(359deg);
    }
}

.iconfont-spin {
    -webkit-animation: an-icon-spin 2s infinite linear;
    animation: an-icon-spin 2s infinite linear;
    display: inline-block;
}

.iconfont-pulse {
    -webkit-animation: an-icon-spin 1s infinite steps(8);
    animation: an-icon-spin 1s infinite steps(8);
    display: inline-block;
}

[class*="an-icon-"] {
    font-family: "an-icon";
    font-size: inherit;
    font-style: normal;
}
